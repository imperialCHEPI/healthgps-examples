                "level": 0,
                "range": [1, 4]
            },
            {
                "name": "Ethnicity",
                "level": 0,
                "range": [1, 4]
            },
            {
                "name": "Income",
                "level": 0,
                "range": [0, 100000000]
            },
            {
                "name": "Carbohydrate",
                "level": 1,
                "range": [50, 500]
            },
            {
                "name": "Sodium",
                "level": 1,
                "range": [500, 5000]
            },
            {
                "name": "Protein",
                "level": 1,
                "range": [20, 200]
