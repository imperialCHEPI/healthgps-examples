                "level": 1,
                "range": [0, 200]
            },
            {
                "name": "AddedSugar",
                "level": 1,
                "range": [0, 150]
            },
            {
                "name": "PhysicalActivity",
                "level": 2,
                "range": [1.388473, 1.835487]
            },
            {
                "name": "EnergyIntake",
                "level": 2,
                "range": [803.2424, 4885.412]
