        "checksum": "186e45d273c51b6ed6b9febda749b245948248b3b3dec97b87967fac4a3aeb52"
    },
    "inputs": {
        "dataset": {
            "name": "Finch.DataFile.csv",
            "format": "csv",
            "delimiter": ",",
            "encoding": "ASCII",
            "columns": {
                "Gender": "integer",
                "Age": "integer",
                "Age2": "integer",
                "Age3": "integer",
                "Region": "integer",
                "Sector": "integer",
                "Income": "integer",
                "Ethnicity": "integer",
                "Carbohydrate": "double",
                "Protein": "double",
                "Sodium": "double",
                "Fat": "double",
                "Alcohol": "double",
                "ProcessedMeat": "double",
                "RedMeat": "double",
                "Fruit": "double",
                "Vegetable": "double",
                "Legume": "double",
                "PolyunsaturatedFattyAcid": "double",
                "SaturatedFat": "double",
                "MonounsaturatedFat": "double",
                "Fibre": "double",
                "TotalSugar": "double",
                "AddedSugar": "double",
                "PhysicalActivity": "double",
                "EnergyIntake": "double",
                "Weight": "double",
                "Height": "double"
            }
        },
        "settings": {
            "country_code": "GBR",
            "size_fraction": 0.000001,
            "age_range": [0, 110]
        }
    },

    "modelling": {
        "ses_model": {
            "function_name": "normal",
