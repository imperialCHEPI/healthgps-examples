{
    "version": 2,
    "inputs": {
        "dataset": {
            "name": "India.DataFile.csv",
            "format": "csv",
            "delimiter": ",",
            "encoding": "ASCII",
            "columns": {
                "Gender": "integer",
                "Age": "integer",
                "Age2": "integer",
                "Age3": "integer",
                "Sector": "integer",
                "Income": "integer",
                "Carbohydrate": "double",
                "Protein": "double",
                "Sodium": "double",
                "Fat": "double",
                "PhysicalActivity": "double",
                "EnergyIntake": "double",
                "Weight": "double",
                "Height": "double"
            }
        },
        "settings": {
            "country_code": "IND",
            "size_fraction": 0.00001,
            "age_range": [0, 100]
        }
    },
    "modelling": {
        "ses_model": {
            "function_name": "normal",
            "function_parameters": [0.0, 1.0]
        },
        "risk_factors": [
            {
                "name": "Gender",
                "level": 0,
                "range": [0, 1]
            },
            {
                "name": "Age",
                "level": 0,
                "range": [0, 100]
            },
            {
                "name": "Carbohydrate",
                "level": 1,
                "range": [120.155, 774.2651]
            },
            {
                "name": "Fat",
                "level": 1,
                "range": [14.65918, 204.6782]
            },
            {
                "name": "Protein",
                "level": 1,
                "range": [20.3546, 126.8509]
            },
            {
                "name": "Sodium",
                "level": 1,
                "range": [984.7666, 8844.316]
            },
            {
                "name": "BMI",
                "level": 3,
                "range": [13.88, 39.48983]
            }
        ],
        "risk_factor_models": {
            "static": "Dummy.json",
            "dynamic": "Dummy.json"
        },
        "baseline_adjustments": {
            "format": "csv",
            "delimiter": ",",
            "encoding": "UTF8",
            "file_names": {
                "factorsmean_male": "FactorsMean.Male.csv",
                "factorsmean_female": "FactorsMean.Female.csv"
            }
        }
    },
    "running": {
        "seed": [12345],
        "start_time": 2022,
        "stop_time": 2030,
        "trial_runs": 1,
        "sync_timeout_ms": 15000,
        "diseases": ["ischemicheartdisease"],
        "interventions": {
            "active_type_id": "simple",
            "types": {
                "simple": {
                    "active_period": {
                        "start_time": 2022,
                        "finish_time": null
                    },
                    "impact_type": "absolute",
                    "impacts": []
                }
            }
        }
    },
    "output": {
        "comorbidities": 5,
        "folder": "${HOME}/healthgps/example_ischemicheartdisease_output",
        "file_name": "HealthGPS_Result_{TIMESTAMP}.json"
    }
}
